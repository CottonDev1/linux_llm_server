{
  "name": "llm-rag-server",
  "version": "1.0.0",
  "description": "RAG server with vector database, llama.cpp backend, and Roslyn C# code analysis",
  "main": "rag-server.js",
  "type": "module",
  "scripts": {
    "start": "node rag-server.js",
    "dev": "node --watch rag-server.js",
    "analyze": "node roslyn-wrapper.js",
    "test": "node --test tests/**/*.test.js",
    "test:browser": "node tests/browser/audio-bulk.test.js",
    "test:browser:headed": "set HEADLESS=false && node tests/browser/audio-bulk.test.js",
    "test:admin": "node tests/browser/admin-full-test.js",
    "test:admin:headed": "HEADLESS=false node tests/browser/admin-full-test.js",
    "test:prefect": "node tests/browser/prefect-pipeline-test.js",
    "test:prefect:headed": "HEADLESS=false node tests/browser/prefect-pipeline-test.js",
    "test:e2e": "npx playwright test",
    "test:e2e:headed": "npx playwright test --headed",
    "test:e2e:ui": "npx playwright test --ui",
    "allure:generate": "npx allure generate allure-results -o allure-report --clean",
    "allure:open": "npx allure open allure-report",
    "allure:serve": "npx allure serve allure-results",
    "playwright:codegen": "npx playwright codegen http://localhost:3000"
  },
  "keywords": [
    "rag",
    "vector-database",
    "code-analysis",
    "roslyn",
    "llama.cpp",
    "semantic-search"
  ],
  "author": "EWR Development Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "prom-client": "^15.1.3",
    "uuid": "^13.0.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "allure-commandline": "^2.36.0",
    "allure-js-commons": "^3.4.3",
    "allure-playwright": "^3.4.3",
    "playwright": "^1.57.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chadmroberts88/LLMRagServer.git"
  },
  "homepage": "https://github.com/chadmroberts88/LLMRagServer#readme",
  "bugs": {
    "url": "https://github.com/chadmroberts88/LLMRagServer/issues"
  }
}
